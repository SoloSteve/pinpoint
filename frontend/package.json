    "name": "Pinpoint",
    "framework": "vue",
    "template": "single-view",
    "cssPreProcessor": false,
    "bundler": "webpack",
    "webpack": {
      "developmentSourceMap": true,
      "productionSourceMap": true,
      "hashAssets": false,
      "preserveAssetsPaths": false,
      "inlineAssets": true
